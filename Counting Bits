class Solution:
    def countBits(self, n: int) -> List[int]:
        count = []
        for i in range(n+1):
            bit = bin(i)
            bit = bit[2:]
            bit = list(bit)
            cnt = bit.count("1")
            count.append(cnt)
        return count            
