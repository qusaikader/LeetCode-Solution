class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        minprice=max(prices)
        maxprofit=0
        for price in prices:
            if price<minprice: 
                minprice=price
            profit = price-minprice
            if profit>maxprofit:
                maxprofit=profit
        return maxprofit

